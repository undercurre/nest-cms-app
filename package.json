{
  "name": "mova-cms-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:uat": "vue-tsc && vite build --mode uat",
    "build:prod": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --skipLibCheck --pretty",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "docker": "docker build -t mova-cms-app .",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "dependencies": {
    "axios": "^1.7.9",
    "country-iso": "^2.0.5",
    "country-iso-3-to-2": "^1.1.1",
    "country-language": "^0.1.7",
    "echarts": "^5.6.0",
    "iso-3166-1-alpha-2": "^1.0.2",
    "leaflet-geosearch": "^4.2.0",
    "markdown-it": "^14.1.0",
    "pinia": "^3.0.0",
    "pinia-plugin-persistedstate": "^4.3.0",
    "reverse-geocode": "^1.3.3",
    "socket.io-client": "^4.8.1",
    "vant": "^4.9.16",
    "vue": "^3.5.13",
    "vue-i18n": "10",
    "vue-router": "^4.5.0",
    "vue3-video-play": "^1.3.2"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vant/auto-import-resolver": "^1.2.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.0.11",
    "jsdom": "^25.0.1",
    "less": "^4.2.2",
    "lint-staged": "^15.2.5",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "unocss": "^65.4.2",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
